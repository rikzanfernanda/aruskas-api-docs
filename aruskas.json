  "paths": {
    "/api/register": {
      "post": {
        "summary": "Register user",
        "tags": [
          "Authentication"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "nama",
                  "email",
                  "password"
                ],
                "properties": {
                  "nama": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  },
                  "pekerjaan": {
                    "type": "string"
                  },
                  "alamat": {
                    "type": "string"
                  }
                }
              },
              "examples": {
                "Bambang": {
                  "description": "Example body",
                  "value": {
                    "nama": "Bambang",
                    "email": "bambang@gmail.com",
                    "password": "bambang",
                    "pekerjaan": "Wiraswasta",
                    "alamat": "Pemalang"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Response success from register",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DefaultResponseSchema"
                },
                "examples": {
                  "Bambang": {
                    "$ref": "#/components/examples/UserResponseExample"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/login": {
      "post": {
        "summary": "Login user",
        "tags": [
          "Authentication"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
