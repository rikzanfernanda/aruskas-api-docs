  "components": {
    "securitySchemes": {
      "AruskasAuth": {
        "description": "Authentication for Aruskas",
        "type": "http",
        "bearerFormat": "JWT",
        "scheme": "bearer"
      }
    },
    "schemas": {
      "DefaultResponseSchema": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          },
          "result": {
            "type": "object"
          }
        }
      },
      "AnggaranRequestSchema": {
        "type": "object",
        "properties": {
          "nama": {
            "type": "string"
          }
        },
        "required": [
          "nama"
        ]
      },
      "PemasukanRequestSchema": {
        "type": "object",
        "properties": {
          "pemasukan": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "jumlah": {
                  "type": "integer"
                },
                "bank": {
                  "type": "integer"
                }
              }
            }
          }
        }
      },
      "PengeluaranRequestSchema": {
        "type": "object",
        "properties": {
          "pengeluaran": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "jumlah": {
                  "type": "integer"
                },
                "bank": {
                  "type": "integer"
                },
                "anggaran": {
                  "type": "integer"
                }
              }
            }
          }
        }
      },
      "PlanRequestSchema": {
        "type": "object",
        "properties": {
          "anggarans_id": {
            "type": "integer"
          },
          "bulan": {
            "type": "integer"
          },
          "tahun": {
            "type": "integer"
          },
          "jumlah": {
            "type": "integer"
          },
          "frekuensi": {
            "type": "integer"
          },
          "satuan": {
            "type": "string"
          },
          "total": {
            "type": "integer"
          }
        }
      },
      "UserRequestSchema": {
        "type": "object",
        "properties": {
          "nama": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "pekerjaan": {
            "type": "string"
          },
          "alamat": {
            "type": "string"
          }
        },
        "required": ["nama", "email"]
      },
      "ChangePasswordRequestSchema": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string"
          }
        },
        "required": ["password"]
      }
    },
